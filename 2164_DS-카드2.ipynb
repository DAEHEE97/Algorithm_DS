   "source": [
    "from collections import deque\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "q = deque(list(range(1, N+1)))\n",
    "\n",
    "while len(q) != 1:\n",
    "    \n",
    "    q.popleft()\n",
    "    \n",
    "    q.append(q.popleft())\n",
    "\n",
    "print(q[0])"
   ]
  }
